{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ES2015", "ES2019", "DOM", ],
    "module": "ES6",
//    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./dist"
  },
  "include": ["./src/**/*.ts" ],
  "exclude": ["node_modules", "dist", "build", "test", "tsconfig.json"]
}